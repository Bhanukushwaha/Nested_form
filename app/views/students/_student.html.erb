<div id="<%= dom_id student %>">
  <p>
    <strong>First name:</strong>
    <%= student.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= student.last_name %>
  </p>

  <p>
    <strong>City:</strong>
    <%= student.city %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= student.description %>
  </p>

  <p>
    <strong>Test:</strong>
    <%= student.test %>
  </p>
  

</div>
